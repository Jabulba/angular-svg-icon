{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "svg",
      "kebab-case"
    ],
    "ban-types": false,
    "no-conflicting-lifecycle": false
  }
}
