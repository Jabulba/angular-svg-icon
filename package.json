  "license": "MIT",
  "typings": "index.d.ts",
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "svg",
    "icon"
  ],
  "scripts": {
    "start": "rimraf runt && npm run dist && tsc && concurrently \"tsc -w\" \"lite-server\" ",
