{
  "name": "angular-svg-icon",
  "description": "Angular 2+ component for inlining SVGs allowing them to be easily styled with CSS.",
  "version": "4.2.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/czeckd/angular-svg-icon.git"
  },
