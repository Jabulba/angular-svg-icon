{
  "name": "angular-svg-icon",
  "description": "Angular 6+ component for inlining SVGs allowing them to be easily styled with CSS.",
  "version": "7.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/czeckd/angular-svg-icon.git"
  },
  "author": "David Czeck",
  "license": "MIT",
  "keywords": [
    "angular",
    "svg",
    "icon"
  ],
  "scripts": {
    "start": "rimraf runt && npm run dist && tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rimraf runt dist",
    "dist": "ng-packagr -p lib/package.json && cp README.md LICENSE dist",
    "lint": "tslint -c tslint.json ./app/**/*.ts ./lib/**/*.ts -t verbose || true",
    "lite": "lite-server"
  },
  "devDependencies": {
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.3.0",
    "concurrently": "^2.2.0",
    "core-js": "^2.5.5",
    "lite-server": "^2.2.2",
    "ng-packagr": "^4.3.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.66.6",
    "rxjs": "^6.0.0",
    "systemjs": "0.19.47",
    "ts-node": "~5.0.1",
    "tsickle": "^0.34.0",
    "tslint": "~5.9.1",
    "typescript": "~3.2.2",
    "zone.js": "^0.8.26"
  }
}
