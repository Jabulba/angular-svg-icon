{
  "name": "angular-svg-icon",
  "description": "Angular 5 component for inlining SVGs allowing them to be easily styled with CSS.",
  "version": "5.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/czeckd/angular-svg-icon.git"
  },
  "author": "David Czeck",
  "license": "MIT",
  "typings": "index.d.ts",
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "svg",
    "icon"
  ],
  "scripts": {
    "start": "rimraf runt && npm run dist && tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rimraf runt dist",
    "dist": "tools/mkdist",
    "lite": "lite-server",
    "lint": "tslint -c tslint.json ./app/**/*.ts ./lib/**/*.ts -t verbose || true"
  },
  "peerDependencies": {
    "@angular/core": "^4.3.0 || ^5.0.0",
    "@angular/common": "^4.3.0 || ^5.0.0",
    "rxjs": "^5.5.2"
  },
  "devDependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@types/core-js": "^0.9.43",
    "@types/node": "^8.0.50",
    "concurrently": "^2.2.0",
    "core-js": "^2.5.1",
    "lite-server": "^2.2.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.51.1",
    "rxjs": "^5.5.2",
    "systemjs": "0.19.47",
    "ts-node": "^3.3.0",
    "tslint": "~5.8.0",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.18"
  }
}
