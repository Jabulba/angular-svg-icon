    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@types/core-js": "^0.9.39",
    "@types/node": "^6.0.60",
    "concurrently": "^2.2.0",
    "core-js": "^2.4.1",
    "lite-server": "^2.2.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rxjs": "^5.2.0",
    "systemjs": "0.19.47",
